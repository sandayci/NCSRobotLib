    "        4: range(64,80),\n",
    "        5: range(80,88),\n",
    "    }\n",
    "\n",
    "    def get_group(n):\n",
    "        for k, v in groups.items():\n",
    "            if n in v:\n",
    "                return k\n",
    "\n",
    "    interval = 500        \n",
    "\n",
    "    rolls_frame = data_rolls\n",
    "    rolls_frame['group'] = rolls_frame['neuron'].apply(get_group)\n",
