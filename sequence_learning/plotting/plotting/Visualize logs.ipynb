    "def get_good_ticks(maximum, num=3):\n",
    "    search = maximum / num\n",
    "    \n",
    "    # get nice number around search\n",
    "    #get the first larger power of 10\n",
    "    nice = int(10**math.ceil(math.log10(search)))\n",
    "    # scale the power to a \"nice enough\" value\n",
    "    if (search < 0.25 * nice):\n",
    "        nice = int(math.ceil(0.25 * nice))\n",
    "    elif (search < 0.5 * nice):\n",
    "        nice = int(math.ceil(0.5 * nice))\n",
    "\n",
    "    ret = list(range(0, num * nice, nice))\n",
    "    if num*nice < maximum:\n",
    "        ret.append(num*nice)\n",
    "    return ret\n",
    "\n",
